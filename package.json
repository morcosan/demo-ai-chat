{
	"name": "demo-ai-chat",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"serve-app": "cross-env NODE_ENV=localhost vite",
		"serve-ds": "cross-env NODE_ENV=localhost storybook dev --no-open -p 9000",
		"build-app-dev": "tsc -b && cross-env NODE_ENV=dev vite build",
		"build-app-alpha": "tsc -b && cross-env NODE_ENV=alpha vite build",
		"build-app-beta": "tsc -b && cross-env NODE_ENV=beta vite build",
		"build-app-prod": "tsc -b && cross-env NODE_ENV=prod vite build",
		"build-ds-dev": "cross-env NODE_ENV=dev storybook build -o __dist-ds",
		"build-ds-prod": "cross-env NODE_ENV=prod storybook build -o __dist-ds",
		"preview-app": "vite preview",
		"preview-ds": "vite preview --config .storybook/vite.config.ts",
		"lint": "eslint .",
		"clear-cache": "rmdir /s /q \"node_modules/.cache\"",
		"uninstall": "rmdir /s /q \"node_modules\""
	},
	"dependencies": {
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.26.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.8.0",
		"@faker-js/faker": "^8.4.1",
		"@storybook/addon-essentials": "^8.2.9",
		"@storybook/react-vite": "^8.2.9",
		"@storybook/theming": "^8.2.9",
		"@types/node": "^22.2.0",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.1",
		"autoprefixer": "^10.4.20",
		"cross-env": "^7.0.3",
		"dotenv-flow": "^4.1.0",
		"eslint": "^9.9.0",
		"eslint-plugin-react-hooks": "^5.1.0-rc.0",
		"eslint-plugin-react-refresh": "^0.4.9",
		"globals": "^15.9.0",
		"postcss": "^8.4.41",
		"prettier": "^3.3.3",
		"prettier-plugin-organize-imports": "^4.0.0",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"storybook": "^8.2.9",
		"tailwindcss": "^3.4.10",
		"tsx": "^4.17.0",
		"typescript": "^5.5.3",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.4.0"
	}
}
