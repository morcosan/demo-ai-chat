{
	"name": "demo-ai-chat",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"app--local": "cross-env NODE_ENV=app-local vite",
		"app--build-dev": "tsc -b && cross-env NODE_ENV=app-dev vite build",
		"app--build-alpha": "tsc -b && cross-env NODE_ENV=app-alpha vite build",
		"app--build-beta": "tsc -b && cross-env NODE_ENV=app-beta vite build",
		"app--build-prod": "tsc -b && cross-env NODE_ENV=app-prod vite build",
		"app--preview": "vite preview",
		"ds--local": "cross-env NODE_ENV=ds-local storybook dev --no-open -p 9000",
		"ds--build-dev": "cross-env NODE_ENV=ds-dev storybook build -o __dist-ds",
		"ds--build-prod": "cross-env NODE_ENV=ds-prod storybook build -o __dist-ds",
		"ds--preview": "vite preview --config .storybook/vite.config.ts",
		"lint": "eslint .",
		"lint-fix": "eslint . --fix",
		"lint-typescript": "tsc -b",
		"clear-cache": "rmdir /s /q \"node_modules/.cache\"",
		"uninstall": "rmdir /s /q \"node_modules\""
	},
	"dependencies": {
		"@emotion/cache": "^11.13.1",
		"@emotion/react": "^11.13.3",
		"@mui/lab": "^6.0.0-beta.9",
		"@mui/material": "^6.0.2",
		"antd": "^5.20.6",
		"clsx": "^2.1.1",
		"lodash": "^4.17.21",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-router-dom": "^6.26.1"
	},
	"devDependencies": {
		"@eslint/js": "^9.8.0",
		"@faker-js/faker": "^8.4.1",
		"@storybook/addon-a11y": "^8.3.1",
		"@storybook/addon-essentials": "^8.3.1",
		"@storybook/addon-toolbars": "^8.3.1",
		"@storybook/react-vite": "^8.3.1",
		"@storybook/theming": "^8.3.0",
		"@types/lodash": "^4.17.7",
		"@types/node": "^22.2.0",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@vitejs/plugin-react": "^4.3.1",
		"autoprefixer": "^10.4.20",
		"cross-env": "^7.0.3",
		"dotenv-flow": "^4.1.0",
		"eslint": "^9.9.1",
		"eslint-plugin-react": "^7.35.0",
		"eslint-plugin-react-hooks": "^5.1.0-rc.0",
		"eslint-plugin-react-refresh": "^0.4.9",
		"globals": "^15.9.0",
		"postcss": "^8.4.41",
		"postcss-import": "^16.1.0",
		"prettier": "^3.3.3",
		"prettier-plugin-css-order": "^2.1.2",
		"prettier-plugin-organize-imports": "^4.0.0",
		"prettier-plugin-tailwindcss": "^0.6.6",
		"storybook": "^8.3.0",
		"tailwindcss": "^3.4.10",
		"tsx": "^4.17.0",
		"typescript": "^5.5.3",
		"typescript-eslint": "^8.0.0",
		"vite": "^5.4.0",
		"vite-plugin-svgr": "^4.2.0"
	}
}
